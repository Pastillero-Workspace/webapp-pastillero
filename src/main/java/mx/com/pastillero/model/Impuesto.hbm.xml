<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="mx.com.pastillero.model.formBeans.Impuesto" table="Impuesto">
      <meta attribute="class-description">
         This class contains IVA.IEPS information 
      </meta>
     <id name="id" type="int" column="id">
         	<generator class="native"/>
      </id>
      <property name="IVA" column="IVA" type="int"/>
      <property name="IEPS1" column="IEPS1" type="int"/>
      <property name="IEPS2" column="IEPS2" type="int"/>
      </class>
</hibernate-mapping>