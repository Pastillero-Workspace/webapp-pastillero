<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="mx.com.pastillero.model.formBeans.Devolucion" table="Devolucion">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="idDevolucion" type="int" column="idDevolucion">
         <generator class="native"/>
      </id>
      <property name="fecha" column="fecha" type="string"/>
      <property name="hora" column="hora" type="string"/>
      <property name="motivo" column="motivo" type="string"/>
      <property name="idUsuario" column="idUsuario" type="int"/>
      <property name="notaReferencia" column="nota_referencia" type="int"/>
      <property name="notaActual" column="nota_actual" type="int"/>
      </class>
</hibernate-mapping>