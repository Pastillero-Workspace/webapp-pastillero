<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="mx.com.pastillero.model.formBeans.Presupuesto" table="Presupuesto">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="idPresupuesto" type="int" column="idPresupuesto">
         <generator class="native"/>
      </id>
      <property name="fecha" column="fecha" type="string"/>
      <property name="hora" column="hora" type="string"/>
      <property name="subtotal" column="subtotal" type="int"/>
      <property name="idUsuario" column="idUsuario" type="int"/>
      </class>
</hibernate-mapping>