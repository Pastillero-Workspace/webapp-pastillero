<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="mx.com.pastillero.model.formBeans.Recepcion" table="Recepcion">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="idRecepcion" type="int" column="idRecepcion">
         <generator class="native"/>
      </id>
      <property name="numFactura" column="num_factura" type="string"/>
      <property name="fecha" column="fecha" type="string"/>
      <property name="hora" column="hora" type="string"/>
      <property name="desc1" column="desc_1" type="float"/>
      <property name="desc2" column="desc_2" type="float"/>
      <property name="folioElectronico" column="folio_electronico" type="int"/>
      <property name="notaFactura" column="nota_factura" type="int"/>
      <property name="subtotal" column="subtotal" type="float"/>
      <property name="iva" column="iva" type="float" />
      <property name="ieps" column="ieps" type="float" />
      <property name="ieps2" column="ieps2" type="float" />
      <property name="total" column="total" type="float" />
      <property name="estado" column="estado" type="int"/>
      <property name="idUsuario" column="idUsuario" type="int"/>
      <property name="idProveedor" column="idProveedor" type="int"/>
      </class>
</hibernate-mapping>